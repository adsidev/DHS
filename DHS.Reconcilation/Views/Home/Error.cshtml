@{
    ViewBag.Title = "DHS Reconcilation :: Error Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<title>@ViewBag.Title</title>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<div class="tab-pane active" id="dashboard">
    <div class="container-fluid">
        <!-- top tiles -->
        <div class="row ">
            <div class="col-lg-12">
                <div class="mrp_content">
                    <h2>View Error Details</h2>
                    <form class="mrp_selectdetails form-inline">
                    </form>
                </div>
            </div>
        </div>
        <div class="form_modal">
            <label style="color:red">There is an error occurred. Please contact adminstrator. To Continue work please click on browser back button.</label>
        </div>
    </div>
</div>
