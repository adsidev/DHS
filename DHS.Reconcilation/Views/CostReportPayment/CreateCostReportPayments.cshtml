                        <div class="col-lg-4 col-xs-12 col-sm-4 col-md-4">
                            <div class="form_modal">
                                <label>Check Date</label>
                            </div>
                        </div>
                        <div class="col-lg-8 col-xs-12 col-sm-8 col-md-8">
                            <div class="form_modal">
                                @Html.TextBoxFor(m => m.CostReportPaymentEntity.CheckDate, htmlAttributes: new { @id = "CheckDate", @class = "form-control mrp_modalselect" })
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="row">
                        <div class="col-lg-4 col-xs-12 col-sm-4 col-md-4">
                            <div class="form_modal">
                                <label>Check Number</label>
                            </div>
                        </div>
                        <div class="col-lg-8 col-xs-12 col-sm-8 col-md-8">
                            <div class="form_modal">
                                @Html.TextBoxFor(m => m.CostReportPaymentEntity.CheckNumber, htmlAttributes: new { @id = "CheckNumber", @class = "form-control mrp_modalselect" })
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="row">
                        <div class="col-lg-4 col-xs-12 col-sm-4 col-md-4">
                            <div class="form_modal">
                                <label>Check Amount</label>
                            </div>
                        </div>
                        <div class="col-lg-8 col-xs-12 col-sm-8 col-md-8">
                            <div class="form_modal">
                                @Html.TextBoxFor(m => m.CostReportPaymentEntity.CheckAmount, htmlAttributes: new { @id = "CheckAmount", Value = String.Format("{0:$#,##0.00}", Model.CostReportPaymentEntity.CheckAmount), @class = "form-control mrp_modalselect" })
                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="col-lg-6">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="form_modal">
                                <label><span class="red">Reviewer Comments:</span></label>
                                <textarea id="txtComments" name="txtComments" style="margin: 0px; width: 500px; height: 60px;">@Model.CostReportPaymentEntity.Comments</textarea>
                            </div>
                        </div>
                    </div>
                </div>



            </div>
           
            <div class="modal-footer">
                <input type="submit" id="btnSubmit" class="btn btn-primary btn-updated" onclick="return SubmitValue()" value="Save Changes" />
            </div>
        </div>
    </div>
</div>
}