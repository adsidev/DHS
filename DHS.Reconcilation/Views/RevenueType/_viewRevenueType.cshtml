@model DHSEntities.RevenueTypeResponse
@{
    ViewBag.Title = "View RevenueTypes";
}

<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="RevenueTypeModalLabel">
                View Revenue Type Information
            </h5>

            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <span class="mrp_modaledit"> <strong><a href="Javascript:void()" onclick="EditRevenueType()" title="Edit"><i class="fa fa-pencil" aria-hidden="true" onclick="EditRevenueType()"></i></a></strong></span>
        </div>
        <div class="modal-body">
            <div class="mrp_modalcontent">
                <input type="hidden" id="RevenueTypeId" name="RevenueTypeId" value="@Model.RevenueTypeEntity.RevenueTypeId" />
                <div class="row">

                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-3 col-xs-12 col-sm-3 col-md-3">
                                <div class="form_modal">
                                    <label>RevenueType&nbsp;Name</label>
                                </div>
                            </div>
                            <div class="col-lg-9 col-xs-12 col-sm-9 col-md-9">
                                <div class="form_modal">
                                    <p><lable id="lbFacilityCharges">@Model.RevenueTypeEntity.RevenueTypeName</lable></p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-3 col-xs-12 col-sm-3 col-md-3">
                                <div class="form_modal">
                                    <label>Description:</label>
                                </div>
                            </div>
                            <div class="col-lg-9 col-xs-12 col-sm-9 col-md-9">
                                <div class="form_modal">
                                    <p>@Model.RevenueTypeEntity.RevenueTypeDescription</p>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
